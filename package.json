    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.4",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.0",
    "redux": "^5.0.0",
    "require": "^0.4.4",
    "socket.io": "^4.7.2",
